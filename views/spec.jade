!!! 5
html
  head
    title JUMLY spec
    link(rel="stylesheet", type="text/css", href="javascripts/jasmine/jasmine.css")
    script(type="text/javascript", src="javascripts/jasmine/jasmine.js")
    script(type="text/javascript", src="javascripts/jasmine/jasmine-html.js")
    script(type="text/javascript", src="javascripts/jquery.js")
    script(type="text/javascript", src="javascripts/coffee-script.js")
    != css("jumly")
    != js("spec/helper")
    != js("jumly")
    != js("spec/legacy/jasmine-story")
    != js("spec/legacy/0.1.0/spec")
    != js("spec/legacy/0.1.1/spec")
    != js("spec/legacy/0.1.2/spec")
    :less
      #TrivialReporter {z-index:10;}
      .suite .suite > .description, .jasmine_reporter > .suite > .description {background-color:transparent;}
      body {font-size:10pt;}
  body
  :coffeescript
    $ ->
      env = jasmine.getEnv()
      env.addReporter new jasmine.TrivialReporter
      env.execute()
